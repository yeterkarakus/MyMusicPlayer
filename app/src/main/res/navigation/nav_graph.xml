<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/searchFragment">

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.yeterkarakus.miniyoutube.view.searchpage.SearchFragment"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_searchFragment_to_searchNotFoundFragment"
            app:destination="@id/searchNotFoundFragment" />
        <action
            android:id="@+id/action_searchFragment_to_showMoreFragment"
            app:destination="@id/showMoreFragment" />
    </fragment>
    <fragment
        android:id="@+id/searchNotFoundFragment"
        android:name="com.yeterkarakus.miniyoutube.view.searchpage.SearchNotFoundFragment"
        android:label="SearchNotFoundFragment" />
    <fragment
        android:id="@+id/showMoreFragment"
        android:name="com.yeterkarakus.miniyoutube.view.searchpage.ShowMoreFragment"
        android:label="fragment_show_more"
        tools:layout="@layout/fragment_show_more" />
</navigation>